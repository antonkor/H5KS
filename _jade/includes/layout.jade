doctype html
html.no-js
block vars

head
  meta(charset="utf-8")
  meta(http-equiv="X-UA-Compatible",content="IE=edge")
  block pageMeta
  
  meta(name="viewport",content="width=device-width, initial-scale=1")

  link(rel="stylesheet" href="assets/css/bootstrap.css")
  link(rel="stylesheet" href="assets/css/global.css")

  block pageHeadEnd

header#top.page-header.navbar.navbar-fixed-top.animated.fadeIn
  div.container
    div.navbar-header
      button.navbar-toggle(type='button', data-toggle='collapse', data-target='.bs-navbar-collapse') 
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.logo-brand(href="index.html")
        img(src='assets/img/logo.png')
    nav.collapse.navbar-collapse.bs-navbar-collapse.pull-right
      ul.nav.navbar-nav
        li
          a(href='index.html') Home
        li
          a(href="about.html") About
        li
          a(href="blog.html") Blog
        li
          a(href="contact.html") Contact
          

body(class="#{bodyClass}")
  block pageBody


footer.page-footer.animated.fadeIn
  div.container
    ul.nav.navbar-nav
      li
        a(href="index.html") Home
      li
        a(href="about.html") About
      li
        a(href="blog.html") Blog
      li
        a(href="contact.html") Contact


<script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="js/vendor/jquery-1.10.2.min.js"><\/script>')</script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
<script src="assets/js/plugins.js"></script>
